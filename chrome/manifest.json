{
	"name":"pageMark",
	"version":"0.0.1",
	"manifest_version": 2,
	"description":"page mark tool for alibaba.com",
	"author":"ginano",
	"homepage_url": "http://www.ginano.net/index.html",
	"browser_action":{
		"default_icon":"icon.png",
		"default_title":"pageMark",
		"popup":"popup.html"
	},
	"permissions":[
		"tabs",
		"*://*.alibaba.com/",
		"*://*.alibaba-inc.com/"
	],
	"options_page":"options.html",
	 "content_scripts": [
		 {
			"matches":[
				"*://*.alibaba.com/",
				"*://*.alibaba-inc.com/"
			],
			"js":["merge.js"]
		 }
	]
	
}
